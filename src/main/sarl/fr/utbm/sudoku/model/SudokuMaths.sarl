package fr.utbm.sudoku.model

import java.util.ArrayList
import java.util.Set

/** 
 * @author Cl√©ment Bareth-Desgranges <clement.bareth@gmail.com>
 * 
 */
capacity SudokuMaths {
	def evaluateCandidates : ArrayList<Integer>
}

skill SudokuMathsBaseSkill implements SudokuMaths{
	
	var values : Set<Integer>
	
	new (v : Set<Integer>) {
		values = v
	}
	
	def evaluateCandidates : ArrayList<Integer> {
		var candidates = new ArrayList<Integer>
		for(var i=1;i<=9;i++){
			if (!values.contains(i)) {
				candidates.add(i)
			}
		}
		return candidates
	}
}

